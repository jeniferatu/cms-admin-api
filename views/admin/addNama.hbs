{{>admin/header}}
<!-- Content Wrapper. Contains page content -->
<div class="content-wrapper">
	<!-- Content Header (Page header) -->
	<section class="content-header">
		<h1>
			Menambahkan Nama 
			<small>menambah nama</small>
		</h1>
		<ol class="breadcrumb">
			<li>
				<a href="/admin">
					<i class="fa fa-dashboard"></i>Home</a>
			</li>
			<li><a href="/admin/daftarnama"><i class="fa fa-inbox"></i>Nama</a></li>
		    <li class="active">Tambah</li>
		</ol>
	</section>

	<!-- Main content -->
	<section class="content container-fluid">
		<div class="container-fluid half-padding">
			<div class="template template__blank">
				<div class="row">
					<!-- flash message -->
					
					
					<div class="col-md-12">
						<div class="box box-primary">
							<div class="box-header with-border">
								<h3 class="box-title">Add Nama Form Information</h3>
							</div>
							<form method="post" action="/admin/createNama">							
								<table class="table table-bordered">
									<tbody>
										<tr>
											<th style="width:50px">id</th>
											<td>
												<input type="text" class="form-control" name="Id" style="width:300px" value="" required>												
											</td>
										</tr>
										<tr>
											<th style="width:50px">nama</th>
											<td>
												<input type="text" class="form-control" name="Nama" style="width:300px" value="" required>			
											</td>
										</tr>
                                        <tr>
											<th style="width:50px">nama_keuangan</th>
											<td>
												<input type="text" class="form-control" name="Nama_Keuangan" style="width:300px" value="" required>												
											</td>
										</tr>
										<tr>
											<th style="width:50px">nama_penjadwalan</th>
											<td>
												<input type="text" class="form-control" name="Nama_Penjadwalan" style="width:300px" value="" required>			
											</td>
										</tr>
                                        <tr>
											<th style="width:50px">nama_bau</th>
											<td>
												<input type="text" class="form-control" name="Nama_BAU" style="width:300px" value="" required>												
											</td>
										</tr>
										<tr>
											<th style="width:50px">gelar</th>
											<td>
												<input type="text" class="form-control" name="Gelar" style="width:300px" value="" required>			
											</td>
										</tr>
										 <tr>
											<th style="width:50px">nik</th>
											<td>
												<input type="text" class="form-control" name="NIK" style="width:300px" value="" required>												
											</td>
										</tr>
										<tr>
											<th style="width:50px">rekening_bank</th>
											<td>
												<input type="text" class="form-control" name="Rekening_Bank" style="width:300px" value="" required>			
											</td>
										</tr>
                                         <tr>
											<th style="width:50px">nama_rekening_bank</th>
											<td>
												<input type="text" class="form-control" name="Nama_Rek_DKI" style="width:300px" value="" required>												
											</td>
										</tr>
										<tr>
											<th style="width:150px"></th>
											<td colspan="2" style="width:250px">											
												<input type="submit" class="btn btn-primary" value="Submit">
											</td>
										</tr>
									</tbody>
								</table>
							</form>
						</div>
					</div>
				</div>
			</div>
		</div>
	</section>
	<!-- /.content -->
</div>
<!-- /.content-wrapper -->
{{>admin/footer}}